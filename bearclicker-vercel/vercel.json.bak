{
  "version": 2,
  "routes": [
    {
      "src": "/(.*)\\.embed",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)\\.html",
      "dest": "/api/$1"
    },
    {
      "src": "/(.+)",
      "dest": "/api/$1"
    },
    {
      "src": "/",
      "dest": "/api/index"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=14400, s-maxage=2592000"
        }
      ]
    }
  ]
}
